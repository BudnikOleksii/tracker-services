    "eslint": "9.39.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "rulesync": "7.6.3",
    "turbo": "2.8.10",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "22.15.0",
    "pnpm": "10.10.0"
  },
  "packageManager": "pnpm@10.10.0"
