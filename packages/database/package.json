{
  "name": "@tracker/database",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node --env-file=.env --import tsx src/migrate.ts",
    "db:seed": "node --env-file=.env --import tsx src/seeds/index.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@nestjs/common": "11.1.14",
    "bcrypt": "6.0.0",
    "drizzle-orm": "0.45.1",
    "pg": "8.18.0"
  },
  "devDependencies": {
    "@tracker/eslint-config": "workspace:*",
    "@tracker/typescript-config": "workspace:*",
    "@types/bcrypt": "6.0.0",
    "@types/pg": "8.16.0",
    "drizzle-kit": "0.31.9",
    "eslint": "9.39.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  }
}
